    "css-loader": "^6.5.1",
    "eslint": "^8.8.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "mini-css-extract-plugin": "^2.5.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "run-node-webpack-plugin": "^1.3.0",
