  "engines": {
    "node": ">=8.9.0"
  },
  "engineStrict": true,
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "prettier --write",
      "eslint --fix"
    ],
    "package.json": [
      "sortpack"
    ]
  }
